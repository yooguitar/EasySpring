<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kh.easy.admin.model.mapper.AdminMapper">

<select id="findAll" resultType="Member">
	SELECT
	       USER_ID userId,
	       USER_PWD userPwd,
	       EMAIL,
	       ROLE,
	       STATUS,
	       ENROLL_DATE enrollDate
	  FROM	
	       MEMBER
	 ORDER
	 	BY
	 	   ENROLL_DATE DESC
</select>



</mapper>